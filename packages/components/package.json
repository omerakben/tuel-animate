{
  "name": "@tuel/components",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:watch-only": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo coverage"
  },
  "dependencies": {
    "@react-three/postprocessing": "^3.0.4",
    "@tuel/gsap": "workspace:*",
    "@tuel/motion": "workspace:*",
    "@tuel/three": "workspace:*",
    "@tuel/tokens": "workspace:*",
    "@tuel/utils": "workspace:*",
    "matter-js": "^0.20.0",
    "motion": "^12.23.12"
  },
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "gsap": ">=3.12.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@testing-library/react": "^14.1.2",
    "@types/matter-js": "^0.20.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react": "^4.2.1",
    "gsap": "^3.12.5",
    "jsdom": "^23.2.0",
    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "three": "^0.179.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
